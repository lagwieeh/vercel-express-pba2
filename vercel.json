{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "./routes",
      "dest": "/expressions"
    },
    {
      "src": "./",
      "dest": "router.js"
    },
    {
      "src": "./",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
        "src": "/(.+)",
        "dest": "public/$1"
    }
  ]
}