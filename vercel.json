{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    
    },
    {
      "src": "/public/(.*)",
      "dest": "public/$1"
    }
    
    
  ]
}